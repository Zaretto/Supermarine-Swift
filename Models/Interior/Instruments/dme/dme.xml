<?xml version="1.0"?>

<!-- distance measuring equipment -->

<PropertyList>

 <path>dme.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>dme-Needle1</object-name>
  <property>instrumentation/tacan/indicated-distance-nm</property>
  
  <interpolation>
   <entry><ind>0</ind><dep>-150</dep></entry>
   <entry><ind>4</ind><dep>-90</dep></entry>
   <entry><ind>10</ind><dep>0</dep></entry>
   <entry><ind>16</ind><dep>90</dep></entry>
   <entry><ind>20</ind><dep>150</dep></entry>
  </interpolation>
 
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation> 
   <type>material</type>
    <emission> 
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
  </animation>


<!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>
 
</PropertyList>
